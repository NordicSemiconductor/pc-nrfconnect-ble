  "files": [
    "dist/",
    "resources/icon.*",
    "icon.png",
    "LICENSE"
  ],
  "devDependencies": {
    "change-case": "^4.1.0",
    "chart.js": "^2.9.3",
    "immutable": "^3.8.2",
    "jquery": "^3.4.1",
    "lodash.throttle": "^4.1.1",
    "mousetrap": "^1.6.3",
    "pc-nrfconnect-shared": "github:NordicSemiconductor/pc-nrfconnect-shared#v4.1.0",
